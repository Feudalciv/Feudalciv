#. TRANS: "W" is worker citizens, as opposed to specialists;
#. * %s will represent the specialist types, for instance "E/S/T"
#: client/agents/cma_fec.c:334
#, c-format
msgid "People (W/%s)"
msgstr "Asukkaita (T/%s)"

# Välilyönti jätetty pois tahallaan, ks. yllä.
#: client/agents/cma_fec.c:348
msgid " happy"
msgstr "onnellista"

#: client/agents/cma_fec.c:359
#, c-format
msgid ""
"Name: %s\n"
"Food:       %10s Gold:    %10s\n"
"Production: %10s Luxury:  %10s\n"
"Trade:      %10s Science: %10s\n"
"\n"
"%*s%s: %s\n"
"          City grows: %s\n"
"Production completed: %s"
msgstr ""
"Nimi: %s\n"
"Ruokaa:    %10s Kultaa:   %10s\n"
"Tuotantoa: %10s Luksusta: %10s\n"
"Kauppaa:   %10s Tiedettä: %10s\n"
"\n"
"%*s%s: %s\n"
"     Kaupungin kasvu: %s\n"
"     Tuotanto valmis: %s"

#: client/agents/cma_fec.c:427
msgid "?cma:Very happy"
msgstr "Erityisen onnellinen"

#: client/agents/cma_fec.c:428
msgid "?cma:Max food"
msgstr "Maksimoi ruoka"

#: client/agents/cma_fec.c:429
msgid "?cma:Max production"
msgstr "Maksimoi tuotanto"

#: client/agents/cma_fec.c:430
msgid "?cma:Max gold"
msgstr "Maksimoi kulta"

#: client/agents/cma_fec.c:431
msgid "?cma:Max science"
msgstr "Maksimoi tiede"

#: client/attribute.c:371
msgid ""
"There has been a CMA error. Your citizen governor settings may be broken."
msgstr "Virhe käskynhaltijaa ladatessa; sen asetuksiin on voinut tulla vikoja."

#: client/attribute.c:375
msgid "Old attributes detected and removed."
msgstr "Löysin ja poistin vanhoja attribuutteja."

#: client/audio.c:122
#, c-format
msgid "Plugin '%s' isn't available. Available are %s"
msgstr "Liitännäinen '%s' ei ole saatavilla, vain %s"

#: client/audio.c:199
msgid "No real audio plugin present."
msgstr "Ääniliitännäistä ei ole saatavilla."

#: client/audio.c:200 client/audio.c:217 client/audio.c:255 client/audio.c:264
msgid "Proceeding with sound support disabled."
msgstr "Jatketaan ilman ääntä."

#: client/audio.c:201
msgid "For sound support, install SDL_mixer"
msgstr "Asenna SDL_mixer saadaksesi äänet toimimaan."

#: client/audio.c:214
msgid "To get sound you need to download a sound set!"
msgstr "Sinun pitää hakea äänisetti saadaksesi äänet toimimaan!"

#: client/audio.c:215
#, c-format
msgid "Get sound sets from <%s>."
msgstr "Äänisettejä löytyy osoitteesta <%s>."

#: client/audio.c:222
#, c-format
msgid ""
"Could not load sound spec-file '%s':\n"
"%s"
msgstr ""
"Ääniasetustiedoston '%s' lataaminen epäonnistui:\\n\n"
"%s"

#: client/audio.c:262
msgid "No real audio subsystem managed to initialize!"
msgstr "Ääniliitännäisen alustus epäonnistui!"

#: client/audio.c:263
msgid "Perhaps there is some misconfiguration or bad permissions."
msgstr "Kenties asetuksissa tai käyttöoikeuksissa on vikaa."

#: client/chatline_common.c:179
msgid ""
"Freeciv is free software and you are welcome to distribute copies of it "
"under certain conditions;"
msgstr ""
"Freeciv on vapaa ohjelmisto; saat mieluusti välittää kopioita siitä "
"tiettyjen ehtojen täyttyessä;"

#: client/chatline_common.c:182
msgid "See the \"Copying\" item on the Help menu."
msgstr "Katso \"Copying\"-kohta Ohje-valikossa."

#: client/chatline_common.c:184
msgid "Now ... Go give 'em hell!"
msgstr "Mene, näe ja voita!"

#: client/chatline_common.c:196
#, c-format
msgid "Exporting output window to '%s' ..."
msgstr "Tulostusikkunan sisältö tallennetaan tiedostoon '%s' ..."

#: client/chatline_common.c:203
msgid "Export complete."
msgstr "Tallennettu."

#: client/chatline_common.c:206
msgid "Export failed, couldn't write to file."
msgstr "Tallennus epäonnistui, tiedostoon ei voi kirjoittaa."

#. TRANS: Use longer of "XXX turns" and "never"
#: client/citydlg_common.c:259
msgid "?filler:XXX/XXX XXX turns"
msgstr "XXX/XXX XXX vuoroa"

#: client/citydlg_common.c:266
#, c-format
msgid "%3d gold per turn"
msgid_plural "%3d gold per turn"
msgstr[0] "%3d kulta joka vuoro"
msgstr[1] "%3d kultaa joka vuoro"

