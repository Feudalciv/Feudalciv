#: data/civ2/terrain.ruleset:797 data/classic/terrain.ruleset:966
#: data/experimental/terrain.ruleset:942 data/civ2civ3/terrain.ruleset:966
#: data/multiplayer/terrain.ruleset:940
msgid "Oil"
msgstr "öljyä"

#: data/civ1/terrain.ruleset:684 data/civ2/terrain.ruleset:737
#: data/classic/terrain.ruleset:912 data/experimental/terrain.ruleset:888
#: data/civ2civ3/terrain.ruleset:912 data/multiplayer/terrain.ruleset:886
#: tools/civmanual.c:294
msgid "Resources"
msgstr "luonnonvaroja"

#: data/civ1/terrain.ruleset:692
msgid "Seals"
msgstr "hylkeitä"

#: data/civ1/terrain.ruleset:752 data/civ2/terrain.ruleset:856
#: data/classic/terrain.ruleset:1025 data/experimental/terrain.ruleset:1001
#: data/civ2civ3/terrain.ruleset:1025 data/multiplayer/terrain.ruleset:999
msgid "Fortress"
msgstr "linnoitus"

#: data/civ1/terrain.ruleset:769
msgid "Fortresses improve defense for land units."
msgstr "Linnoitukset kohentavat maavoimien yksiköiden puolustuskykyä."

#: data/civ1/terrain.ruleset:902 data/civ2/terrain.ruleset:1030
