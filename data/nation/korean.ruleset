[nation_korean]

name=_("Korean")
plural=_("?plural:Koreans")
groups=_("Modern"), _("Medieval"), _("Asian")
legend=_("According to legend, the first Korean kingdom was founded in\
 prehistory by the ancestral Tangun in southern Manchuria. The Korean\
 kingdoms was united in 668 by king Munmu. Today the Korean peninsula is\
 divided into two states, North and South Korea as a result of the stalemate\
 after the Korean war in 1953.")
leader=
 "Tangun",	; Ancestral founder of Korea
 "Kwanggaet'o",	; Great king of Goguryeo, 4th century
 "Sondok",	; Queen of Silla, 7th century
 "Munmu",	; King of Silla - unified the Korean kingdoms
 "Wang Kon",	; Founder and king of Goryeo, temple name "T'aejo"
 "Yi Song-gye",	; Founder and king of Joseon, temple name "T'aejo"
 "T'aejong",	; King of Joseon
 "Sejong",	; Great king of Joseon
 "Min",		; Queen of Joseon - fiercely opposed Japanese oppression
 "Kim Il-sung",	; Eternal leader of North Korea
 "Syngman Rhee",	; First president of South Korea
 "Park Chung Hee",	; Second president of South Korea
 "Kim Dae-jung"		; Fifth president of South Korea
leader_sex= "Male", "Male", "Female", "Male", "Male", "Male", "Male", "Male",
            "Female", "Male", "Male", "Male", "Male"
flag="korea"
flag_alt = "-"
city_style = "Asian"

ruler_titles = { "government",      "male_title",        "female_title"
                 "Communism",       _("Great Leader"),   _("Great Leader")
               }

