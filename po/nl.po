msgstr "Ruiters"

#: data/civ1/units.ruleset:467 data/civ2/units.ruleset:487
#: data/default/units.ruleset:487
msgid "Chariot"
msgstr "Strijdwagens"

#: data/civ1/units.ruleset:490 data/civ2/units.ruleset:510
#: data/default/units.ruleset:510
msgid "Elephants"
msgstr "Olifanten"

#: data/civ1/units.ruleset:513 data/civ2/units.ruleset:533
#: data/default/units.ruleset:533
msgid "Crusaders"
msgstr "Kruisvaarders"

#: data/civ1/units.ruleset:536 data/civ2/units.ruleset:556
#: data/default/units.ruleset:556
msgid "Knights"
msgstr "Ridders"

#: data/civ1/units.ruleset:560 data/civ2/units.ruleset:580
#: data/default/units.ruleset:580
msgid "Dragoons"
msgstr "Huzaren"

#: data/civ1/units.ruleset:444 data/civ2/units.ruleset:603
#: data/default/units.ruleset:603
msgid "Cavalry"
msgstr "Cavalerie"

#: data/civ1/units.ruleset:606 data/civ2/units.ruleset:626
#: data/default/units.ruleset:626
msgid "Armor"
msgstr "Tank"

#: data/civ1/units.ruleset:629 data/civ2/units.ruleset:649
#: data/default/units.ruleset:649
msgid "Catapult"
