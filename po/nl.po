#: data/default/units.ruleset:620 data/civ2/units.ruleset:504
#: data/history/units.ruleset:544
msgid "Paratroopers"
msgstr "Paratroepen"

#: data/default/units.ruleset:652 data/civ2/units.ruleset:536
#: data/civ1/units.ruleset:286 data/history/units.ruleset:576
msgid "Mech. Inf."
msgstr "Grenadiers"

#: data/default/units.ruleset:678 data/civ2/units.ruleset:562
#: data/civ1/units.ruleset:312 data/history/units.ruleset:602
msgid ""
"Mechanized Infantry; this unit has the strongest defence strength of any "
"land unit, but is only available near the end of the technology tree."
msgstr ""
"Pantser Grenadiers; deze eenheid heeft de grootste verdedigingssterkte van "
"alle landeenheden, maar is pas tegen het einde van de technologische "
"ontwikkeling beschikbaar."

#: data/default/units.ruleset:685 data/civ2/units.ruleset:569
#: data/history/units.ruleset:609
msgid "Horsemen"
msgstr "Ruiters"

#: data/default/units.ruleset:713 data/civ2/units.ruleset:597
#: data/civ1/units.ruleset:347 data/history/units.ruleset:637
msgid "Chariot"
msgstr "Strijdwagens"

#: data/default/units.ruleset:741 data/civ2/units.ruleset:681
#: data/civ1/units.ruleset:375 data/history/units.ruleset:665
msgid "Knights"
msgstr "Ridders"

#: data/default/units.ruleset:770 data/civ2/units.ruleset:710
#: data/history/units.ruleset:694
msgid "Dragoons"
msgstr "Huzaren"

#: data/default/units.ruleset:799 data/civ2/units.ruleset:739
#: data/civ1/units.ruleset:319 data/history/units.ruleset:723
msgid "Cavalry"
msgstr "Cavalerie"

#: data/default/units.ruleset:827 data/civ2/units.ruleset:767
#: data/civ1/units.ruleset:404 data/history/units.ruleset:751
