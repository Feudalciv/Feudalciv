msgstr "Dyktator"

#: data/nation/catalan.ruleset:17
msgid "Dictadora"
msgstr "Dyktatorka"

#: data/nation/catalan.ruleset:18
msgid "Rei"
msgstr "Król"

#: data/nation/catalan.ruleset:18
msgid "Reina"
msgstr "Królowa"

