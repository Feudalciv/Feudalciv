#: server/civserver.c:127 client/civclient.c:128
#, c-format
msgid ""
"Usage: %s [option ...]\n"
"Valid options are:\n"
msgstr ""
"Ús: %s [opció ...]\n"
"Les opcions vàlides son:\n"

#: server/civserver.c:129 client/civclient.c:132
msgid "  -d, --debug NUM\tSet debug log level (0 to 4, or 4:file1,min,max:...)\n"
msgstr ""
"  -d, --debug NÚM\tFixa nivell registre depuració (0 a 4, ó 4:fitx1,mín,"
"màx:...)\n"

#: server/civserver.c:132 client/civclient.c:135
msgid "  -d, --debug NUM\tSet debug log level (0 to 3)\n"
msgstr "  -d, --debug NÚM\tFixa nivell registre depuració (0 a 3)\n"

#: server/civserver.c:134
msgid "  -f, --file FILE\tLoad saved game FILE\n"
msgstr "  -f, --file FITXER\tCarrega un joc guardar del FITXER\n"

#: server/civserver.c:135
msgid "  -g, --gamelog FILE\tUse FILE as game logfile\n"
msgstr "  -g, --gamelog FITXER\tUsa FITXER com a fitxer de registre del joc\n"

#: server/civserver.c:136 client/civclient.c:137
msgid "  -h, --help\t\tPrint a summary of the options\n"
msgstr "  -h, --help\t\tImprimeix un resum de les opcions\n"

#: server/civserver.c:137
msgid "  -i, --info INFO\tExtra info for the metaserver\n"
msgstr "  -i, --info INFO\tInformació extra per el metaservidor\n"

#: server/civserver.c:138 client/civclient.c:138
msgid "  -l, --log FILE\tUse FILE as logfile\n"
msgstr "  -l, --log FITXER\tUsa FITXER com a fitxer de registre\n"

#: server/civserver.c:139
msgid "  -m, --meta\t\tNotify metaserver and send server's info\n"
msgstr "  -m, --meta\t\tnotifica el metaservidor i enviar informació servidor\n"

#: server/civserver.c:140
msgid "  -M, --Metaserver ADDR\tSet ADDR as metaserver address\n"
msgstr "  -M, --Metaserver ADDR\tEstableix ADDR com adreça del metaservidor\n"

#: server/civserver.c:142
msgid "  -p, --port PORT\tListen for clients on port PORT\n"
msgstr "  -p, --port PORT\tEscolta als clients al port PORT\n"

#: server/civserver.c:143
msgid "  -q, --quitidle TIME\tQuit if no players for TIME seconds\n"
msgstr "  -q, --quitidle TEMPS\tSurt si no hi ha jugadors en TEMPS segons\n"

#: server/civserver.c:144
msgid "  -r, --read FILE\tRead startup script FILE\n"
msgstr "  -r, --read FITXER\tLlegeix el script FITXER d'inici\n"

#: server/civserver.c:145 client/civclient.c:149
msgid "  -v, --version\t\tPrint the version number\n"
msgstr "  -v, --version\t\tImprimeix la versió\n"

