#, c-format
msgid ""
"Usage: %s [option ...]\n"
"Valid options are:\n"
msgstr ""
"Használat: %s [opció ...]\n"
"Elfogadott opciók:\n"

#: client/civclient.c:105 server/civserver.c:243
msgid "  -h, --help\t\tPrint a summary of the options\n"
msgstr "  -h, --help\t\tÖsszes opció kiírása\n"

#: server/civserver.c:244
msgid "  -r, --read FILE\tRead startup script FILE\n"
msgstr ""

#: server/civserver.c:245
msgid "  -f, --file FILE\tLoad saved game FILE\n"
msgstr "  -f, --file FILE\tJátékállást betölt FILE-ból\n"

#: server/civserver.c:246
msgid "  -p, --port PORT\tListen for clients on port PORT\n"
msgstr "  -p, --port=PORT\tPORT porton vár a kliensekre.\n"

#: server/civserver.c:247
msgid "  -g, --gamelog FILE\tUse FILE as game logfile\n"
msgstr "  -g, --gamelog FILE\tFILE logfájl használata\n"

#: client/civclient.c:106 server/civserver.c:248
msgid "  -l, --log FILE\tUse FILE as logfile\n"
msgstr "  -l, --log FILE\tFILE használata logfájlként.\n"

#: server/civserver.c:249
msgid "  -m, --meta\t\tSend info to metaserver\n"
msgstr "  -m, --meta\t\tInformációt küld a metaszervernek\n"

#: server/civserver.c:250
msgid "  -M, --Metaserver ADDR\tSet ADDR as metaserver address\n"
msgstr "  -M, --Metaserver ADDR\tADDR-t állítja be metaszerver címnek\n"

#: server/civserver.c:251
msgid "  -s, --server HOST\tList this server as host HOST\n"
msgstr "  -s, --server HOST\tEzt a szervert HOST szerverként listázza ki\n"

#: client/civclient.c:116 server/civserver.c:253
msgid ""
"  -d, --debug NUM\tSet debug log level (0,1,2,3,or 3:file1,min,max:...)\n"
msgstr ""
"  -d, --debug NUM\tHibakeresési szint NUM-ra állítása (0,1,2,3,vagy "
"3:file1,min,max:...)\n"

#: client/civclient.c:119 server/civserver.c:256
