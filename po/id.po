msgstr "Rawa-rawa terbentuk dari luapan air, membuat pertanian agak bermasalah."

#: data/civ1/terrain.ruleset:553
#: data/default/terrain.ruleset:678
#: data/multiplayer/terrain.ruleset:678
msgid "Tundra"
msgstr "Tundra"

#: data/civ1/terrain.ruleset:583
#: data/default/terrain.ruleset:711
#: data/multiplayer/terrain.ruleset:711
msgid "Tundra are broad, cold regions, fit for some agriculture and little else."
msgstr "Tundra adalah wilayah yang luas dan dingin, cocok untuk pertanian dan lainnya."

#: data/civ1/terrain.ruleset:616
#: data/default/terrain.ruleset:752
#: data/multiplayer/terrain.ruleset:752
msgid "?animals:Game"
msgstr "Buruan"

#: data/civ1/terrain.ruleset:624
#: data/default/terrain.ruleset:770
#: data/multiplayer/terrain.ruleset:770
msgid "Coal"
msgstr "Batubara"

#: data/civ1/terrain.ruleset:632
#: data/default/terrain.ruleset:778
#: data/multiplayer/terrain.ruleset:778
msgid "Fish"
msgstr "Ikan"

#: data/civ1/terrain.ruleset:640
#: data/default/terrain.ruleset:795
#: data/multiplayer/terrain.ruleset:795
msgid "Gems"
msgstr "Permata"

#: data/civ1/terrain.ruleset:648
msgid "Horses"
msgstr "Kuda"

#: data/civ1/terrain.ruleset:656
#: data/default/terrain.ruleset:827
