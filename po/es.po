#: server/civserver.c:325 client/client_main.c:331 manual/civmanual.c:477
#, c-format
msgid ""
"Usage: %s [option ...]\n"
"Valid options are:\n"
msgstr ""
"Uso: %s [opción ...]\n"
"Las opciones válidas son:\n"

#: server/civserver.c:326
msgid ""
"  -A  --Announce PROTO\tAnnounce game in LAN using protocol PROTO (IPv4/IPv6/"
"none)\n"
msgstr ""
"  -A  --Announce PROTO\tAnuncia la partida en la LAN usando el protocolo "
"PROTO (IPv4/IPv6/none)\n"

#: server/civserver.c:329
msgid ""
"  -D  --Database FILE\tEnable database connection with configuration from "
"FILE.\n"
msgstr "  -D  --Database FICH\tHabilita la conexión a base de datos con la configuración en el fichero FICH.\n"

#: server/civserver.c:331
msgid "  -a  --auth\t\tEnable server authentication (requires --Database).\n"
msgstr "  -a  --auth\t\tHabilita la autentificación en el servidor (requiere --Database).\n"

#: server/civserver.c:333
msgid "  -G  --Guests\t\tAllow guests to login if auth is enabled.\n"
msgstr ""
"  -G  --Guests\t\tPermitir entrar a los invitados si la autentificación está "
"habilitada.\n"

#: server/civserver.c:335
msgid "  -N  --Newusers\tAllow new users to login if auth is enabled.\n"
msgstr ""
"  -N  --Newusers\tPermtir entrar a los nuevos usuarios si la autentificación "
"está habilitada.\n"

#: server/civserver.c:338
msgid "  -b  --bind ADDR\tListen for clients on ADDR\n"
msgstr "  -b  --bind DIRE\tEsperar conexiones de clientes en la dirección DIRE\n"

#: server/civserver.c:339
msgid "  -B  --Bind-meta ADDR\tConnect to metaserver from this address\n"
msgstr "  -B, --Bind-meta DIR\tConecta al metaservidor desde esta dirección\n"

#: server/civserver.c:341 client/client_main.c:336 manual/civmanual.c:479
#, c-format
msgid "  -d, --debug NUM\tSet debug log level (%d to %d, or %d:file1,min,max:...)\n"
msgstr ""
"  -d, --debug NUM\tEstablecer el nivel de depuración (de %d a %d, o %d:fich1,"
"min,max:...)\n"

#: server/civserver.c:345 client/client_main.c:340 manual/civmanual.c:483
#, c-format
msgid "  -d, --debug NUM\tSet debug log level (%d to %d)\n"
msgstr "  -d, --debug NUM\tEstablecer el nivel de depuración (de %d a %d)\n"

#: server/civserver.c:349 client/client_main.c:344 manual/civmanual.c:487
msgid "  -F, --Fatal [SIGNAL]\tRaise a signal on failed assertion\n"
msgstr "  -F, --Fatal [SEÑAL]\tLanzar una señal de assert errónea\n"

#: server/civserver.c:352
msgid "  -f, --file FILE\tLoad saved game FILE\n"
msgstr "  -f, --file FICH\tCargar la partida guardada en FICH\n"

#: server/civserver.c:354 client/client_main.c:348 manual/civmanual.c:490
#: modinst/mpcmdline.c:55
msgid "  -h, --help\t\tPrint a summary of the options\n"
msgstr "  -h, --help\t\tImprimir un resumen de las opciones\n"

#: server/civserver.c:355
msgid "  -i, --identity ADDR\tBe known as ADDR at metaserver\n"
msgstr "  -i, --identity ADDR\tSer conocido como ADDR en el metaserver\n"

#: server/civserver.c:356 manual/civmanual.c:491
msgid "  -l, --log FILE\tUse FILE as logfile\n"
msgstr "  -l, --log FICH\tUsar FICH como fichero de registro\n"

#: server/civserver.c:357
msgid "  -m, --meta\t\tNotify metaserver and send server's info\n"
msgstr "  -m, --meta\t\tEnviar información del servidor al metaservidor\n"

#: server/civserver.c:359
msgid "  -M, --Metaserver ADDR\tSet ADDR as metaserver address\n"
msgstr "  -M, --Metaserver DIR\tEstablecer DIR como dirección del servidor\n"

#: server/civserver.c:362
msgid "  -p, --port PORT\tListen for clients on port PORT\n"
msgstr "  -p, --port PUERTO\tEsperar conexiones de clientes al puerto PUERTO\n"

#: server/civserver.c:364
msgid "  -q, --quitidle TIME\tQuit if no players for TIME seconds\n"
msgstr "  -q, --quitidle SEGS\tSalir si no hay jugadores en SEGS segundos\n"

#: server/civserver.c:366
msgid "  -e, --exit-on-end\tWhen a game ends, exit instead of restarting\n"
msgstr ""
"  -e, --exit-on-end\tCuando una partida acaba, acabar en lugar de empezar de "
"nuevo\n"

#: server/civserver.c:369
msgid "  -s, --saves DIR\tSave games to directory DIR\n"
msgstr "  -s, --saves DIR\tGraba las partidas en el directorio DIR\n"

#: server/civserver.c:371
msgid "  -S, --Serverid ID\tSets the server id to ID\n"
msgstr "  -S, --Serverid ID\tAsigna al servidor id a ID\n"

#: server/civserver.c:372
msgid "  -r, --read FILE\tRead startup script FILE\n"
msgstr "  -r, --read FICH\tEjecutar el fichero de secuencia de inicio FICH\n"

#: server/civserver.c:374
msgid "  -R, --Ranklog FILE\tUse FILE as ranking logfile\n"
msgstr "  -R, --Ranklog FICH\tUsar FICH como fichero de ranking\n"

#: server/civserver.c:377
msgid "  -L, --LoadAI MODULE\tLoad ai module MODULE. Can appear multiple times\n"
msgstr "  -L, --LoadAI MODULO\tCarga el módulode IA MODULO. Puede aparecer múltiples veces\n"

