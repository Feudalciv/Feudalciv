msgstr "Fusileros"

#: data/civ1/units.ruleset:369 data/civ2/units.ruleset:386
#: data/default/units.ruleset:386
msgid "Marines"
msgstr "Marines"

#: data/civ1/units.ruleset:392 data/civ2/units.ruleset:409
#: data/default/units.ruleset:409
msgid "Paratroopers"
msgstr "Paracaidistas"

#: data/civ1/units.ruleset:415 data/civ2/units.ruleset:436
#: data/default/units.ruleset:436
msgid "Mech. Inf."
msgstr "Infantería Mecanizada"

#: data/civ1/units.ruleset:436 data/civ2/units.ruleset:457
#: data/default/units.ruleset:457
msgid ""
"Mechanized Infantry; this unit has the strongest defence strength of any "
"land unit, but is only available near the end of the technology tree."
msgstr ""
"La infantería Mecanizada es la unidad con mayor poder de defensa de las "
"unidades terrestres, sólo está disponible al final del árbol de "
"descubrimientos."

#: data/civ2/units.ruleset:464 data/default/units.ruleset:464
msgid "Horsemen"
msgstr "Hombre a caballo"

#: data/civ1/units.ruleset:466 data/civ2/units.ruleset:487
#: data/default/units.ruleset:487
msgid "Chariot"
msgstr "Carro"

#: data/civ1/units.ruleset:489 data/civ2/units.ruleset:510
#: data/default/units.ruleset:510
msgid "Elephants"
