msgid "waiting"
msgstr "esperando"

#: client/plrdlg_common.c:184 client/gui-sdl/plrdlg.c:657
msgid "done"
msgstr "acabado"

#: client/plrdlg_common.c:186 client/gui-sdl/plrdlg.c:659
msgid "moving"
msgstr "moviendo"

#: client/plrdlg_common.c:229
msgid "?Player:Name"
msgstr "Nombre"

#: client/plrdlg_common.c:230
