#: client/civclient.c:110 server/civserver.c:122
#, c-format
msgid ""
"Usage: %s [option ...]\n"
"Valid options are:\n"
msgstr ""
"Uso: %s [opción ...]\n"
"Las opciones válidas son:\n"

#: client/civclient.c:111 server/civserver.c:123
msgid "  -h, --help\t\tPrint a summary of the options\n"
msgstr "  -h, --help            Imprimir un resumen de las opciones\n"

#: server/civserver.c:124
msgid "  -r, --read FILE\tRead startup script FILE\n"
msgstr "  -r, --read FICH       Ejecutar el fichero de preparación FICH\n"

#: server/civserver.c:125
msgid "  -f, --file FILE\tLoad saved game FILE\n"
msgstr "  -f, --file FICH       Cargar la partida guardada FICH\n"

#: server/civserver.c:126
msgid "  -p, --port PORT\tListen for clients on port PORT\n"
msgstr "  -p, --port PUERTO     Esperar conexiones de clientes al puerto PUERTO\n"

#: server/civserver.c:127
msgid "  -g, --gamelog FILE\tUse FILE as game logfile\n"
msgstr "  -g, --gamelog FICH    Usar FICH como fichero de registro del juego\n"

#: client/civclient.c:112 server/civserver.c:128
msgid "  -l, --log FILE\tUse FILE as logfile\n"
msgstr "  -l, --log FICH        Usar FICH como fichero de registro\n"

#: server/civserver.c:129
msgid "  -m, --meta\t\tSend info to metaserver\n"
msgstr "  -m, --meta            Enviar información al metaservidor\n"

#: server/civserver.c:130
msgid "  -M, --Metaserver ADDR\tSet ADDR as metaserver address\n"
msgstr "  -M, --Metaserver DIR  Establecer DIR como dirección del servidor\n"

