#: data/classic/terrain.ruleset:355 data/default/terrain.ruleset:363
msgid "Gold"
msgstr "Oro"

#: data/civ2/terrain.ruleset:369 data/default/terrain.ruleset:369
msgid "Iron"
msgstr "Hierro"

#: data/civ1/terrain.ruleset:377 data/civ2/terrain.ruleset:385
#: data/classic/terrain.ruleset:377 data/default/terrain.ruleset:385
msgid ""
"Mountains are regions of extreme altitude, making agriculture and trade very "
"difficult."
msgstr ""
"Las Montañas son regiones de gran altitud, lo cual hace que la agricultura y "
"el comercio sean muy dificiles."

#: data/civ1/terrain.ruleset:383 data/civ2/terrain.ruleset:391
#: data/classic/terrain.ruleset:383 data/default/terrain.ruleset:391
msgid "Ocean"
msgstr "Océano"

#: data/civ1/terrain.ruleset:391 data/civ2/terrain.ruleset:399
#: data/classic/terrain.ruleset:391 data/default/terrain.ruleset:399
