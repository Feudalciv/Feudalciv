#: data/civ1/units.ruleset:536 data/civ2/units.ruleset:556
#: data/default/units.ruleset:556
msgid "Knights"
msgstr "Ritter"

#: data/civ1/units.ruleset:560 data/civ2/units.ruleset:580
#: data/default/units.ruleset:580
msgid "Dragoons"
msgstr "Dragoner"

#: data/civ1/units.ruleset:444 data/civ2/units.ruleset:603
#: data/default/units.ruleset:603
