#: client/civclient.c:110 server/civserver.c:125
#, c-format
msgid ""
"Usage: %s [option ...]\n"
"Valid options are:\n"
msgstr ""
"Användning: %s [valmöjlighet ...]\n"
"Giltiga valmöjligheter är:\n"

#: client/civclient.c:111 server/civserver.c:126
msgid "  -h, --help\t\tPrint a summary of the options\n"
msgstr "  -h , --help\t\tSkriv en sammanfattning av valmöjligheterna\n"

#: server/civserver.c:127
msgid "  -r, --read FILE\tRead startup script FILE\n"
msgstr "  -r, --read FIL\tLäs FIL innehållande skript vid programkörningens början\n"

#: server/civserver.c:128
msgid "  -f, --file FILE\tLoad saved game FILE\n"
msgstr "  -f, --file FIL\tLadda sparat spel FIL\n"

#: server/civserver.c:129
msgid "  -p, --port PORT\tListen for clients on port PORT\n"
msgstr "  -p, --port PORT\tLyssna efter klienter på port PORT\n"

#: server/civserver.c:130
msgid "  -g, --gamelog FILE\tUse FILE as game logfile\n"
msgstr "  -g, --gamelog FIL\tAnvänd FIL som spelloggningsfil\n"

#: client/civclient.c:112 server/civserver.c:131
msgid "  -l, --log FILE\tUse FILE as logfile\n"
msgstr "  -l, --log FIL\tAnvänd FIL som loggningsfil\n"

#: server/civserver.c:132
msgid "  -m, --meta\t\tSend info to metaserver\n"
msgstr "  -m, --meta\t\tSkicka information till metaservern\n"

#: server/civserver.c:133
msgid "  -M, --Metaserver ADDR\tSet ADDR as metaserver address\n"
msgstr "  -M, --Metaserver ADRESS\tSätt ADRESS som metaserveradress\n"

#: server/civserver.c:134
msgid "  -s, --server HOST\tList this server as host HOST\n"
msgstr "  -s, --server VÄRD\tLista denna server som VÄRD\n"

#: client/civclient.c:122 server/civserver.c:136
msgid "  -d, --debug NUM\tSet debug log level (0 to 4, or 4:file1,min,max:...)\n"
msgstr "  -d, --debug NUMMER\tSätt avlusningsloggningsläget (0 till 4, eller 4:file1,min,max:...)\n"

#: client/civclient.c:125 server/civserver.c:139
msgid "  -d, --debug NUM\tSet debug log level (0 to 3)\n"
msgstr "  -d, --debug NUMMER\tSätt avlusningsloggningsläge (0 till 3)\n"

#: client/civclient.c:127 server/civserver.c:141
msgid "  -v, --version\t\tPrint the version number\n"
msgstr "  -v, --verison\t\tSkriv versionsnumret\n"

