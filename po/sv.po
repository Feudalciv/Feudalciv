#: data/default/units.ruleset:865 data/civ2/units.ruleset:800
#: data/civ1/units.ruleset:774 data/history/units.ruleset:867
msgid "Cannon"
msgstr "kanon"

#: data/default/units.ruleset:893 data/civ2/units.ruleset:828
#: data/civ1/units.ruleset:830 data/history/units.ruleset:895
msgid "Artillery"
msgstr "artilleri"

#: data/default/units.ruleset:921 data/civ2/units.ruleset:856
#: data/history/units.ruleset:923
msgid "Howitzer"
msgstr "haubits"

#: data/default/units.ruleset:949 data/civ2/units.ruleset:884
#: data/civ1/units.ruleset:858 data/history/units.ruleset:951
