#: data/nation/viking.ruleset:32 data/nation/welsh.ruleset:22
msgid "Prime Minister"
msgstr "Premier Ministre"

#: data/nation/american.ruleset:3
msgid "American"
msgstr "Américain"

#: data/nation/american.ruleset:4
msgid "?plural:Americans"
msgstr "?plural:Américains"

#: data/nation/american.ruleset:6
msgid ""
"Formally named the United States of America.  Independent from Great Britain "
"after a revolution in 1776-1783AD."
msgstr ""
"Officiellement nommés les États Unis d'Amérique. Indépendants de la Grande "
"Bretagne après une révolution (1776-1783 ap. J.-C.)."

#: data/nation/american.ruleset:64 data/nation/scottish.ruleset:17
msgid "Speaker"
msgstr "Prêcheur"

#: data/nation/american.ruleset:65
msgid "Reverend"
msgstr "Révérend"

#: data/nation/arab.ruleset:3
msgid "Arab"
msgstr "Arabe"

#: data/nation/arab.ruleset:4
msgid "?plural:Arabs"
msgstr "?plural:Arabes"

#: data/nation/arab.ruleset:5 data/nation/assyrian.ruleset:5
#: data/nation/aztec.ruleset:5 data/nation/babylonian.ruleset:5
#: data/nation/bavarian.ruleset:5 data/nation/boer.ruleset:5
