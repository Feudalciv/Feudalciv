#: client/civclient.c:104 server/civserver.c:242
#, c-format
msgid ""
"Usage: %s [option ...]\n"
"Valid options are:\n"
msgstr ""
"Utilisation: %s [option ...]\n"
"Les options valides sont:\n"

#: client/civclient.c:105 server/civserver.c:243
msgid "  -h, --help\t\tPrint a summary of the options\n"
msgstr "  -h, --help\t\tAfficher un résumé des options\n"

#: server/civserver.c:244
msgid "  -r, --read FILE\tRead startup script FILE\n"
msgstr "  -r, --read F   \tLire le script de démarrage F\n"

#: server/civserver.c:245
msgid "  -f, --file FILE\tLoad saved game FILE\n"
msgstr "  -f, --file F   \tCharger un jeu sauvegardé dans F\n"

#: server/civserver.c:246
msgid "  -p, --port PORT\tListen for clients on port PORT\n"
msgstr "  -p, --port P   \tÉcouter les clients sur le port P\n"

#: server/civserver.c:247
msgid "  -g, --gamelog FILE\tUse FILE as game logfile\n"
msgstr "  -g, --gamelog FICHIER\tSauver les logs de la partie dans FICHIER\n"

#: client/civclient.c:106 server/civserver.c:248
msgid "  -l, --log FILE\tUse FILE as logfile\n"
msgstr "  -l, --log FICHIER\tSauver les logs dans FICHIER\n"

#: server/civserver.c:249
msgid "  -m, --meta\t\tSend info to metaserver\n"
msgstr "  -m, --meta\t\tEnvoyer l'information au Métaserveur\n"

#: server/civserver.c:250
msgid "  -M, --Metaserver ADDR\tSet ADDR as metaserver address\n"
msgstr "  -M, --Metaserver A\tRègle l'adresse du metaserveur sur A\n"

#: server/civserver.c:251
msgid "  -s, --server HOST\tList this server as host HOST\n"
msgstr "  -s, --server H   \tDéclarer ce serveur comme l'hôte H\n"

#: client/civclient.c:116 server/civserver.c:253
msgid ""
"  -d, --debug NUM\tSet debug log level (0,1,2,3,or 3:file1,min,max:...)\n"
msgstr ""
"  -d, --debug NUM\tChoisir le niveau de débogage (0,1,2,3,ou "
"3:fichier1,min,max:...)\n"

#: client/civclient.c:119 server/civserver.c:256
msgid "  -d, --debug NUM\tSet debug log level (0,1,2)\n"
msgstr "  -d, --debug N  \tChoisir le niveau de débogage (0,1,2)\n"

#: client/civclient.c:121 server/civserver.c:258
msgid "  -v, --version\t\tPrint the version number\n"
msgstr "  -v, --version\t\tAfficher le numéro de version\n"

