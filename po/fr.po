#: data/nation/brazilian.ruleset:20 data/nation/chinese.ruleset:22
msgid "Chairman"
msgstr "Président"

#: data/nation/brazilian.ruleset:20 data/nation/chinese.ruleset:22
msgid "Chairperson"
msgstr "Présidente"

