msgid "  -M, --Metaserver ADDR\tSet ADDR as metaserver address\n"
msgstr "  -M, --Metaserver A\t\tRègle l'adresse du métaserveur sur A\n"

#: server/civserver.c:144
msgid "  -p, --port PORT\tListen for clients on port PORT\n"
msgstr "  -p, --port P\t\t\tÉcouter les clients sur le port P\n"

