msgid "  -q, --quitidle TIME\tQuit if no players for TIME seconds\n"
msgstr ""
" -q, --quitidle SEC\t\tQuitter s'il n'y a pas de joueurs pendant SEC "
"secondes\n"

#: server/civserver.c:146
msgid "  -r, --read FILE\tRead startup script FILE\n"
msgstr "  -r, --read F\t\t\tLire le script de démarrage F\n"

