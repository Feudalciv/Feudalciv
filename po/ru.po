#, fuzzy, c-format
msgid "Inka %s"
msgstr "Ханква"

#: data/nation/inca.ruleset:33
#, fuzzy, c-format
msgid "Qoya %s"
msgstr "Коя"

#: data/nation/inca.ruleset:34
#, fuzzy, c-format
msgid "Sapa Inka %s"
msgstr "Верховный Инка"

