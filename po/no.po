#: client/civclient.c:105 server/civserver.c:247
#, c-format
msgid ""
"Usage: %s [option ...]\n"
"Valid options are:\n"
msgstr ""
"Bruk: %s [opsjon ...]\n"
"Gyldige opsjoner er:\n"

#: client/civclient.c:106 server/civserver.c:248
msgid "  -h, --help\t\tPrint a summary of the options\n"
msgstr "  -h, --help\t\tSkriv ut sammendrag av opsjonene\n"

#: server/civserver.c:249
msgid "  -r, --read FILE\tRead startup script FILE\n"
msgstr "  -r, --read FILE\tLes oppstartsmanus i FILE\n"

#: server/civserver.c:250
msgid "  -f, --file FILE\tLoad saved game FILE\n"
msgstr "  -f, --file FILE\tLast inn lagret spill FILE\n"

#: server/civserver.c:251
msgid "  -p, --port PORT\tListen for clients on port PORT\n"
msgstr "  -p, --port PORT\tKople til PORT\n"

#: server/civserver.c:252
msgid "  -g, --gamelog FILE\tUse FILE as game logfile\n"
msgstr "  -g, --gamelog FILE\tBruk FILE som loggfil for spillet\n"

#: client/civclient.c:107 server/civserver.c:253
msgid "  -l, --log FILE\tUse FILE as logfile\n"
msgstr "  -l, --log FILE\tBruk FILE som loggfil\n"

#: server/civserver.c:254
msgid "  -m, --meta\t\tSend info to metaserver\n"
msgstr "  -m, --meta\t\tSend info til metaserveren\n"

#: server/civserver.c:255
msgid "  -M, --Metaserver ADDR\tSet ADDR as metaserver address\n"
msgstr "  -M, --Metaserver ADDR\tSett adressen for Metaserveren\n"

#: server/civserver.c:256
msgid "  -s, --server HOST\tList this server as host HOST\n"
msgstr "  -s, --server HOST\tList denne tjeneren som host HOST\n"

#: client/civclient.c:117 server/civserver.c:258
msgid "  -d, --debug NUM\tSet debug log level (0,1,2,3,or 3:file1,min,max:...)\n"
msgstr "  -d, --debug NUM\t\tSett debuggingsloggnivå (0,1,2,3,eller 3:fil1,min,maks:...)\n"

#: client/civclient.c:120 server/civserver.c:261
