msgid "green"
msgstr "Helt fersk"

#: data/default/units.ruleset:24 data/civ2/units.ruleset:18
#: data/civ1/units.ruleset:18 data/history/units.ruleset:26
msgid "veteran"
msgstr "veteran"

#: data/default/units.ruleset:24 data/history/units.ruleset:26
