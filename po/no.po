#, c-format
msgid ""
"Usage: %s [option ...]\n"
"Valid options are:\n"
msgstr ""
"Bruk: %s [opsjon ...]\n"
"Gyldige opsjoner er:\n"

#: client/civclient.c:111 server/civserver.c:125
msgid "  -h, --help\t\tPrint a summary of the options\n"
msgstr "  -h, --help\t\tSkriv ut sammendrag av opsjonene\n"

#: server/civserver.c:126
msgid "  -r, --read FILE\tRead startup script FILE\n"
msgstr "  -r, --read FILE\tLes oppstartsmanus i FILE\n"

#: server/civserver.c:127
msgid "  -f, --file FILE\tLoad saved game FILE\n"
msgstr "  -f, --file FILE\tLast inn lagret spill FILE\n"

#: server/civserver.c:128
msgid "  -p, --port PORT\tListen for clients on port PORT\n"
msgstr "  -p, --port PORT\tKoble til PORT.\n"

#: server/civserver.c:129
msgid "  -g, --gamelog FILE\tUse FILE as game logfile\n"
msgstr "  -g, --gamelog FILE\tBruk FILE som loggfil for spillet\n"

#: client/civclient.c:112 server/civserver.c:130
msgid "  -l, --log FILE\tUse FILE as logfile\n"
msgstr "  -l, --log FILE\tBruk FILE som loggfil.\n"

#: server/civserver.c:131
msgid "  -m, --meta\t\tSend info to metaserver\n"
msgstr "  -m, --meta\t\tSend info til metaserveren\n"

#: server/civserver.c:132
msgid "  -M, --Metaserver ADDR\tSet ADDR as metaserver address\n"
msgstr "  -M, --Metaserver ADDR Sett adressen for Metaserveren.\n"

#: server/civserver.c:133
msgid "  -s, --server HOST\tList this server as host HOST\n"
msgstr "  -s, --server H\tList denne serveren som H\n"

#: server/civserver.c:134
msgid "  -q, --quitidle TIME\tQuit if no players for TIME seconds\n"
msgstr ""

#: client/civclient.c:122 server/civserver.c:136
msgid ""
"  -d, --debug NUM\tSet debug log level (0 to 4, or 4:file1,min,max:...)\n"
msgstr "  -d, --debug NUM\t\tSett debuggings-loggnivå (0-4, eller 4:fil1,min,max:...)\n"

#: client/civclient.c:125 server/civserver.c:139
msgid "  -d, --debug NUM\tSet debug log level (0 to 3)\n"
msgstr "  -d, --debug NUM\t\tSett debug-loggnivå (0-3)\n"

#: client/civclient.c:127 server/civserver.c:141
msgid "  -v, --version\t\tPrint the version number\n"
msgstr "  -v, --version\t\tSkriv ut versjonsnummeret\n"

