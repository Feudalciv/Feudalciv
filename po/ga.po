msgstr "?female:Cathaoirleach"

#: data/nation/un.ruleset:24
msgid "Secretary-General"
msgstr "Ard-Rúnaí"

#: data/nation/un.ruleset:24
msgid "?female:Secretary-General"
msgstr "?female:Ard-Rúnaí"

#: data/nation/un.ruleset:25
