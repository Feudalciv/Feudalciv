#, c-format
msgid "Server: bad address: <%s:%d>."
msgstr ""

#: server/sernet.c:968
msgid "Cannot open IPv6 socket, trying IPv4 instead"
msgstr ""

#: server/sernet.c:971
#, c-format
msgid "IPv4 service lookup failed <%d>."
msgstr ""

